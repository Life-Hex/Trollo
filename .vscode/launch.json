  "version": "0.2.0",
  "configurations": [
    
  {
      "name": "Launch index.html",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/index.html"
  },
    {
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    }
  ]
}